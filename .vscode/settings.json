{
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "json"
  ],
  "prettier.requireConfig": true,
  "prettier.printWidth": 80,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "eslint.useFlatConfig": true
}
